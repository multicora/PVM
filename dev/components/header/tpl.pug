header
  .navigation-components
    a.navigation(href='')
      i.fa.fa-bars(aria-hidden="true")
    img.logo.pointer(
      src='/files/images/logo_bizkonect.png'
      ng-click="redirect('/library')")
  .navigation-components.right
    div.auth-buttons.inlineBlock
      span.pointer(
        ng-if="vm.isAuthenticated"
        ng-click="vm.invalidateSession()")
        | Logout
    .photo.pointer(ng-click="redirect('/profile')")